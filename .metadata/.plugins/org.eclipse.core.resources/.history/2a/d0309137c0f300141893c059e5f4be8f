
public class Analyse {

	String nom_fichier
	
	public Analyse(String fichier){
		
	}
	
	/**
	 * Fonction de Hashage
	 * 
	 * @param c : Caractère à analyser
	 * @return place dans l'alphabet de c
	 */
	public int Hashage(char c){
		int place = (int)c;
		if(place<=122 & place>=97){
			return (place-96);
		}
		return 27;
	}
	
}
