/**
 * Classe Vidéothèque, représente un ensemble de ressources
 * @author Kilian Cuny
 * @author Guillaume Haben
 * 
 * @version 1.0
 */

import java.util.*;

public class Videotheque {

	// Table des films
	HashMap<Integer, ArrayList<Ressource>> tab_film;
	
	// Table des séries
	HashMap<Integer, ArrayList<Ressource>> tab_serie;
	
	public Videotheque(){
		tab_film = new HashMap<Integer, ArrayList<Ressource>>();
		tab_serie = new HashMap<Integer, ArrayList<Ressource>>();
	}
	
	
}
